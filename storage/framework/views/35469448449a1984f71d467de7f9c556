    		                            	<?php echo Form::text('fname[]',$datas['fname'][$i], ['class'=>'form-control', ]); ?>

                                            <?php endif; ?>
    		                            </div>
    		                            <div class="col-md-3 col-sm-6 clearleft">
    		                            	<label>Middle Name</label>
                                            <?php if(Auth::check() && $i==0): ?>
