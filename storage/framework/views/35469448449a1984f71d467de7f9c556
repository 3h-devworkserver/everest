                                            <?php else: ?>
    		                            	<?php echo Form::text('mname[]',$datas['mname'][$i], ['class'=>'form-control']); ?>

                                            <?php endif; ?>
    		                            </div>
    		                            <div class="col-md-3 col-sm-6">
    		                            	<label>Last Name <em>*</em></label>
                                            <?php if(Auth::check() && $i==0): ?>
                                            <?php echo Form::text('lname[]',Auth::user()->lname, ['class'=>'form-control', 'readonly' ]); ?>

